<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tinovation.androidapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.tinovation.androidapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.tinovation.androidapp.SignupActivity" />
        <activity android:name="com.example.tinovation.androidapp.HomeScreen" />
        <activity android:name="com.example.tinovation.androidapp.NoteDetailActivity" />
        <activity android:name="com.example.tinovation.androidapp.ClassDetailActivity" />
        <activity android:name="com.example.tinovation.androidapp.ProfileActivity" />
        <activity android:name="com.example.tinovation.androidapp.CommentsActivity" />
        <activity android:name="com.example.tinovation.androidapp.AddClassActivity"></activity>
    </application>

</manifest>